public class SumOfSquareRoots {

    public static double calculateSum(int n) {
        double sum = 0.0;
        for (int i = 1; i <= n; i++) {
            sum += Math.sqrt(i);
        }
        return sum;
    }

    public static void main(String[] args) {
        // Testing the function with n = 5
        int n = 5;
        double result = calculateSum(n);
        System.out.println("Sum of square roots for n=" + n + ": " + result);
    }
}
