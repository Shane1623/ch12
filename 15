public class FloatToStringExample {
    public static void main(String[] args) {
        // Convert the floating-point number to a string
        double floatValue = 123.45;
        String stringValue = String.valueOf(floatValue);

        // Print the string and its length
        System.out.println("Converted String: " + stringValue);
        System.out.println("Length of the String: " + stringValue.length());
    }
}
