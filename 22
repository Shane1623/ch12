public class TrigonometricFunction {
    public static void main(String[] args) {
        double result = calculateSinCosSum();
        System.out.println("Result: " + result);
    }

    public static double calculateSinCosSum() {
        double angle1 = Math.PI / 6;  // π/6 radians
        double angle2 = Math.PI / 3;  // π/3 radians

        double sinValue = Math.sin(angle1);
        double cosValue = Math.cos(angle2);

        return sinValue + cosValue;
    }
}
